    @section Scripts{

        <script src="~/js/EtiquetasJS/FormulacionCreate.js"></script>
        <script src="~/js/EtiquetasJS/IngredientesCreateEdit.js"></script>
        <script>
            $( document ).ready(function() {
                @if (Model != null)
                {
                    @foreach(var item in Model.Ingredientes){
                        @:addIngrediente("@Html.Raw(item.Ingrediente.Name)"+" - "+"@item.Ingrediente.Provedor" ,@item.IngredienteId,@item.Percentage);
                    }
                    @: updatePogressBar();

                    @: $("#idFormula").val(@Model.Id);
                    @: $("#nameFormula").val("@Html.Raw(Model.Name)");
                    @: $("#porcionesFormulacion").val(@Model.PorcionId);
                    @if (Model.Densidad.HasValue){
                        @: $("#inputDencidadFormulacion").val(@Model.Densidad);
                    }
                    @:changeDropDownFormulacion();
                }
            });
        </script>


    }
