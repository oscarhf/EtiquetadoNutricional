                        </div>



                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Potasio != null ? "checked" : "") name="potasio" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Potasio</span>

                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Magnesio != null ? "checked" : "") name="magnesio" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Magnesio</span>

                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Cobre != null ? "checked" : "") name="cobre" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Cobre</span>

                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Selenio != null ? "checked" : "") name="selenio" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Selenio</span>

                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2" @(Model.Cromo != null ? "checked" : "") name="cromo" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Cromo</span>

                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2" @(Model.Molibdeno != null ? "checked" : "") name="molibdeno" type="checkbox" value="True">
