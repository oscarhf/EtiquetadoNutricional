


@{
    ViewData["Title"] = "Eliga los elementos a mostrar";
}

@model Etiquetas.Models.Proximales;

<div class="card card-chart" style="min-height: 208px;">

    <div class="card-body">

        <form id="data" action="/Etiqueta/get" method="get">

            <input name="id" type="hidden" value=@Model.Id>

            <div class="card card-chart">

                <div class="card-body">
                    <div class="row">
                        
                        <div class="col-3">
                            <div class="row">
                                <input class="col-2" name="va" @(Model.Va != null ? "checked" : "") type="checkbox" value="True">
                                <span class="col-10 labelcustom">Vitamina A</span>
                            </div>
                        </div>




                        <div class="col-3">
                            <div class="row">
                                <input class="col-2" @(Model.Vb1 != null ? "checked" : "") name="vb1" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Vitamina B1</span>

                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">

                                <input class="col-2" @(Model.Vb2 != null ? "checked" : "") name="vb2" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Vitamina B2</span>
                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2" @(Model.Vb3 != null ? "checked" : "") name="vb3" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Vitamina B3</span>


                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2" @(Model.Vb5 != null ? "checked" : "") name="vb5" type="checkbox" value="True">

                                <span class="col-10 labelcustom">Vitamina B5</span>

                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Vb7 != null ? "checked" : "") name="vb7" type="checkbox" value="True">

                                <span class="col-10 labelcustom">Vitamina B7</span>

                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Vb9 != null ? "checked" : "") name="vb9" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Vitamina B9</span>


                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Vb12 != null ? "checked" : "") name="vb12" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Vitamina B12</span>


                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Vc != null ? "checked" : "") name="vc" type="checkbox" value="True">

                                <span class="col-10 labelcustom">Vitamina C</span>

                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Vd != null ? "checked" : "") name="vd" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Vitamina D</span>
                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Ve != null ? "checked" : "") name="ve" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Vitamina E</span>


                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Vk != null ? "checked" : "") name="vk" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Vitamina K</span>


                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="card card-chart">

                <div class="card-body">
                    <div class="row ">
                        <div class="col-3">
                            <div class="row">

                                <input class="col-2  " @(Model.Calcio != null ? "checked" : "") name="calcio" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Calcio</span>

                            </div>
                        </div>
                        <div class="col-3">
                            <div class="row">

                                <input class="col-2  " @(Model.Fosforo != null ? "checked" : "") name="fosforo" type="checkbox" value="True">
                                <span class="col-10 labelcustom">Fosforo</span>
                            </div>
                        </div>

                        <div class="col-3">
                            <div class="row">
                                <input class="col-2  " @(Model.Fluor != null ? "checked" : "") name="fluor" type="checkbox" value="True">
